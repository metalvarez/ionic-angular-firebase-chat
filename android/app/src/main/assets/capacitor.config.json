{
  "appId": "com.firebase_chat.example",
  "appName": "IonicFirebaseChatApp",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "plugins": {
    "CapacitorFirebaseAuth": {
      "providers": [
        "google.com",
        "twitter.com",
        "facebook.com",
        "phone"
      ],
      "languageCode": "en",
      "nativeAuth": true
    }
  }
}
